{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "extends": ["@schematics/angular"],
  "schematics": {
    "application": {
      "description": "new application workspace",
      "factory": "./application/index#application",
      "schema": "../node_modules/@schematics/angular/application/schema.json",
      "hidden": true
    },
    "workspace": {
      "description": "new workspace",
      "factory": "./workspace/index#workspace",
      "schema": "../node_modules/@schematics/angular/workspace/schema.json",
      "hidden": true
    },
    "ng-new": {
      "description": "create new project",
      "factory": "./ng-new",
      "schema": "../node_modules/@schematics/angular/ng-new/schema.json",
      "hidden": true
    },
    "monaca-component": {
      "aliases": [ "mc" ],
      "description": "Create an Monaca component",
      "factory": "./monaca-component/index#monacaComponent",
      "schema": "../node_modules/@schematics/angular/component/schema.json",
      "hidden": false
    }
  }
}
