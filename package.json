{
  "name": "@monaca/schematics",
  "version": "0.0.0",
  "description": "Monaca Anguler schematics",
  "scripts": {
    "postinstall": "npm install @angular/cli@next --no-save",
    "clean": "del-cli src/**/*.js src/**/*.js.map src/**/*.d.ts !src/*/files/**/*.js",
    "build": "npm run clean && tsc -p tsconfig.json",
    "w": "tsc -p tsconfig.json -w",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "checkformat": "prettier src/*/files/**/*.js --list-different",
    "format": "prettier src/*/files/**/*.js --write",
    "e2e": "del-cli e2e/_tmp e2e/_build && mkdir e2e/_tmp && tsc -p e2e/tsconfig.json && jest --maxWorkers=1 e2e/_build/test"
  },
  "keywords": [
    "schematics",
    "monaca",
    "angular"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MSakamaki/-monaca-schematics"
  },
  "author": "",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "engines": {
    "node": ">=8.9",
    "npm": "~5.6.0node"
  },
  "bugs": {
    "url": "https://github.com/MSakamaki/-monaca-schematics/issues"
  },
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "latest",
    "@angular-devkit/schematics": "latest",
    "@schematics/angular": "latest",
    "@types/jasmine": "^2.8.6",
    "@types/node": "^9.6.2",
    "jasmine": "^2.8.6",
    "monaca-lib": "^2.6.1",
    "typescript": "~2.6.2"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "cheerio": "^1.0.0-rc.2",
    "cpx": "^1.5.0",
    "del-cli": "^1.1.0",
    "jest": "^22.4.3",
    "prettier": "^1.11.1"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "tmp"
    ]
  }
}
