{
  "name": "@monaca/schematics",
  "version": "0.0.1",
  "description": "A Monaca schematics",
  "scripts": {
    "clean": "del 'src/**/*.js' 'src/**/*.js.map' 'src/**/*.d.ts' '!src/*/files/**/*.js'",
    "build": "npm run clean && tsc -p tsconfig.json",
    "w": "tsc -p tsconfig.json -w",
    "test": "npm run clean && npm run build && jasmine src/**/*_spec.js"
  },
  "keywords": [
    "schematics",
    "monaca",
    "angular"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^0.4.8",
    "@angular-devkit/schematics": "^0.4.8",
    "@schematics/angular": "^0.4.8",
    "@nrwl/schematics": "^0.9.1",
    "@types/jasmine": "^2.6.0",
    "@types/node": "^8.0.31",
    "jasmine": "^2.8.0",
    "typescript": "2.6.2"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "del-cli": "^1.1.0"
  }
}
