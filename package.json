{
  "name": "@monaca/schematics",
  "version": "0.0.1",
  "description": "Monaca Anguler schematics",
  "scripts": {
    "tsc:v": "node_modules/.bin/tsc --version",
    "clean": "del-cli src/**/*.js src/**/*.js.map src/**/*.d.ts !src/*/files/**/*.js",
    "build": "npm run clean && node_modules/.bin/tsc -p tsconfig.json",
    "w": "node_modules/.bin/tsc -p tsconfig.json -w",
    "test": "npm run build && jasmine src/**/*_spec.js"
  },
  "keywords": [
    "schematics",
    "monaca",
    "angular"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MSakamaki/-monaca-schematics"
  },
  "author": "",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "bugs": {
    "url": "https://github.com/MSakamaki/-monaca-schematics/issues"
  },
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^0.4.8",
    "@angular-devkit/schematics": "^0.4.8",
    "@schematics/angular": "^0.4.8",
    "@nrwl/schematics": "^0.9.1",
    "@types/jasmine": "^2.6.0",
    "@types/node": "^8.0.31",
    "jasmine": "^2.8.0",
    "typescript": "2.6.2"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "del-cli": "^1.1.0"
  }
}
